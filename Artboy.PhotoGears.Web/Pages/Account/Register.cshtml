@page
@model Artboy.PhotoGears.Web.Pages.Account.RegisterModel
<h5 class="btn-primary text-white text-center p-2">Register- Create a neww account</h5>
<form method="post">
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group">
        <label>User Name</label>
        <input asp-for="UserName" class="form-control"/>
    </div>
    <div class="form-group">
        <label>Email</label>
        <input asp-for="Email" class="form-control"/>
    </div>
    <div class="form-group">
        <label>Password</label>
        <input asp-for="Password" type="password" class="form-control"/>
    </div>
    <div class="form-group">
        <label>Confirm Password</label>
        <input asp-for="ConfirmedPassword" type="password" class="form-control"  />
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
    <a class="btn btn-secondary" asp-page="list">Back</a>
</form>
@{
}
